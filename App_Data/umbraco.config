      <TextPage id="1068" key="3d82fded-3928-4c68-b5be-bf3dd2b358a3" parentID="1065" level="3" creatorID="0" sortOrder="8" createDate="2017-08-05T18:10:33" updateDate="2017-08-09T21:14:25" nodeName="Masterclasses" urlName="masterclasses" path="-1,1064,1065,1068" isDoc="" nodeType="1063" creatorName="Hilary Sharp" writerName="Hilary Sharp" writerID="0" template="1058" nodeTypeAlias="TextPage">
        <content><![CDATA[{
  "name": "1 column layout",
